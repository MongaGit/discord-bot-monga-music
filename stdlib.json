{
  "name": "poleza/discordbot-gamerlobby",
  "timeout": 10000,
  "connector": false,
  "events": {
    "functions/events/discord/message/button/join/player1.js": {
      "name": "discord.message.button.interaction",
      "subtype": {
        "custom_id": "join-player-1"
      }
    },
    "functions/events/discord/message/button/join/player2.js": {
      "name": "discord.message.button.interaction",
      "subtype": {
        "custom_id": "join-player-2"
      }
    },
    "functions/events/discord/message/button/join/leave.js": {
      "name": "discord.message.button.interaction",
      "subtype": {
        "custom_id": "leavegame"
      }
    },
    "functions/events/discord/command/Start.js": {
      "name": "discord.command",
      "subtype": {
        "command": "startgame"
      }
    }
  },
  "env": [
    {
      "name": "Title",
      "description": "Title of Game Start Embed"
    },
    {
      "name": "Description",
      "description": "Description of Game Start Embed"
    },
    {
      "name": "game",
      "description": "Endpoint Link to the game"
    }
  ]
}